<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.edgesoft.resulthour.edesoft">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.edgesoft.resulthour.com.edgesoft.resulthour.DashboardActivity"
            android:theme="@style/CustomAppTheme"/>
        <activity
            android:name="com.edgesoft.resulthour.com.edgesoft.resulthour.ResultWebViewActivity"
            android:theme="@style/CustomAppTheme" />
        <activity
            android:name="com.edgesoft.resulthour.com.edgesoft.resulthour.ViewResultActivity"
            android:theme="@style/CustomAppTheme" />
        <activity
            android:name="com.edgesoft.resulthour.com.edgesoft.resulthour.SearchResultActivity"
            android:theme="@style/CustomAppTheme" />
        <activity
            android:name="com.edgesoft.resulthour.com.edgesoft.resulthour.HomeActivity"
            android:theme="@style/CustomAppTheme" />
        <activity android:name="com.edgesoft.resulthour.com.edgesoft.resulthour.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>